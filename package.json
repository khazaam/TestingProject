{
  "name": "testingproject",
  "version": "1.0.0",
  "description": "[![Coverage Status](https://coveralls.io/repos/github/khazaam/TestingProject/badge.svg?branch=master)](https://coveralls.io/github/khazaam/TestingProject?branch=master)",
  "nyc": {
	"check-coverage": true,
	"include": [
		"src/**.js"
	],
	"reporter": [
		"lcov",
		"text-summary"
	]
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha test --recursive ./test --ignore 'test/app.spec.js'",
    "start": "env NODE_ENV=test node ./src/*",
    "coverage": "nyc npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  },
  "type": "module",
  "dependencies": {
    "jest": "^26.6.3"
  },
  "directories": {
    "test": "test",
    "src": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khazaam/TestingProject.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/khazaam/TestingProject/issues"
  },
  "homepage": "https://github.com/khazaam/TestingProject#readme"
}
